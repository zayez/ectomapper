{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "lib": [""],
  "include": ["src/**/*", "test/models", "test/test.ts"],
  "exclude": ["node_modules", "**/__tests__/*", "**/*.spec.ts"]
}
